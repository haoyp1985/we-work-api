      x-run-in-apifox: https://app.apifox.com/web/project/2640672/apis/api-138509384-run
components:
  schemas:
    SyncRoomInfoModel:
      title: SyncRoomInfoModel
      required:
        - guid
        - room_id
      type: object
      properties:
        guid:
          title: Guid
          type: string
        room_id:
          title: Room Id
          type: string
        version:
          title: Version
          type: integer
          default: 0
      x-apifox-orders:
        - guid
        - room_id
        - version
      x-apifox-ignore-properties: []
      x-apifox-folder: ''
    HTTPValidationError:
      title: HTTPValidationError
      type: object

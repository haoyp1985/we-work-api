      title: SetNotifyMsgFlagModel
      required:
        - guid
        - self_msg
        - contact_msg
        - self_room_msg
        - contact_room_msg
      type: object
      properties:
        guid:
          title: Guid
          type: string
        self_msg:
          title: Self Msg
          type: boolean

# 开发环境配置
NODE_ENV=development

#启动端口
VITE_PORT=15000

# API 基础URL
VITE_API_BASE_URL=http://localhost:18080/api


# 应用标题
VITE_APP_TITLE=WeWork AI Agent Management Platform

# 是否开启调试模式
VITE_DEBUG=true

VITE_BYPASS_AUTH=true

# 文件上传大小限制 (MB)
VITE_UPLOAD_SIZE_LIMIT=10

# 支持的图片格式
VITE_IMAGE_FORMATS=jpg,jpeg,png,gif,webp

# 支持的文件格式
VITE_FILE_FORMATS=pdf,doc,docx,xls,xlsx,ppt,pptx,txt,md

# 是否开启Mock数据
VITE_USE_MOCK=false

# WebSocket地址
VITE_WS_URL=ws://localhost:8080/ws

# 是否启用PWA
VITE_PWA_ENABLED=false

# 版本信息
VITE_APP_VERSION=1.0.0

# 构建时间戳
VITE_BUILD_TIME=development
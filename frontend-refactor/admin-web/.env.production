# 生产环境配置
NODE_ENV=production

# API 基础URL (生产环境需要替换为实际的API地址)
VITE_API_BASE_URL=https://api.wework-ai.com/api

# 应用标题
VITE_APP_TITLE=WeWork AI Agent Management Platform

# 是否开启调试模式
VITE_DEBUG=false

# 文件上传大小限制 (MB)
VITE_UPLOAD_SIZE_LIMIT=10

# 支持的图片格式
VITE_IMAGE_FORMATS=jpg,jpeg,png,gif,webp

# 支持的文件格式
VITE_FILE_FORMATS=pdf,doc,docx,xls,xlsx,ppt,pptx,txt,md

# 是否开启Mock数据
VITE_USE_MOCK=false

# WebSocket地址 (生产环境需要替换为实际的WebSocket地址)
VITE_WS_URL=wss://api.wework-ai.com/ws

# 是否启用PWA
VITE_PWA_ENABLED=true

# 版本信息
VITE_APP_VERSION=1.0.0

# CDN 基础URL (可选，用于静态资源加速)
VITE_CDN_BASE_URL=https://cdn.wework-ai.com

# 是否启用Sentry错误监控
VITE_SENTRY_ENABLED=true
VITE_SENTRY_DSN=https://your-sentry-dsn

# 是否启用Google Analytics
VITE_GA_ENABLED=true
VITE_GA_ID=GA_MEASUREMENT_ID